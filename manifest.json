{
	"name": "Quick Stats",
	"version": "1.0",
	"manifest_version": 2,
	"description" : "Gives desired field results when searched by any of its attributes.",
	"icons" : { "48" : "icon.png"},
	"content_scripts": [
    {
      "matches": ["https://coinmarketcap.com/*"],
      "js": ["script.js"],
      "css": ["CS.css"]
    }
  ],
	"browser_action" :{ 
	"default_title" :  "Quick Stats",
	"default_icon" : "icon.png"
	}
}
